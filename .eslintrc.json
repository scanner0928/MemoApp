{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "comma-dangle": 0,
    "import/prefer-default-export": 0,
    "no-use-before-define": 0,
    "react/prop-types": [
      "error",
      { "ignore": ["navigation"] }
    ]
  }
}
